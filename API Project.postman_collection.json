{
	"info": {
		"_postman_id": "8599e316-bf39-4a97-ae53-c45fd52e8694",
		"name": "API Project",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "8581706"
	},
	"item": [
		{
			"name": "Create New Organization",
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "{{BaseUrl}}/1/organizations?displayName={displayName}&key={{APIKey}}&token={{APIToken}}",
					"host": [
						"{{BaseUrl}}"
					],
					"path": [
						"1",
						"organizations"
					],
					"query": [
						{
							"key": "displayName",
							"value": "{displayName}"
						},
						{
							"key": "key",
							"value": "{{APIKey}}"
						},
						{
							"key": "token",
							"value": "{{APIToken}}"
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "get your member id",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "https://api.trello.com/1/members/me?key={{APIKey}}&token={{APIToken}}",
					"protocol": "https",
					"host": [
						"api",
						"trello",
						"com"
					],
					"path": [
						"1",
						"members",
						"me"
					],
					"query": [
						{
							"key": "key",
							"value": "{{APIKey}}"
						},
						{
							"key": "token",
							"value": "{{APIToken}}"
						}
					]
				}
			},
			"response": []
		}
	]
}